{
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",
		"baseUrl": ".",
		"esModuleInterop": true,
		"paths": {
			"@/*": ["src/*"]
		},
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"skipLibCheck": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"types": ["node"]
	}
}
